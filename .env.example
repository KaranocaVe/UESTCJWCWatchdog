# 复制为 .env：cp .env.example .env
#
# 必填：教务系统账号/密码（导入 .env 或 CLI 运行都需要）
account=202XXXXXXXXXXXX
password="your_password_here"

# ===== 可选：仅 CLI 使用的环境变量（也可用命令行参数覆盖）=====
# 注意：这些配置是通过“进程环境变量”读取的；如果你写在 .env 里，
# 需要在运行前导出（macOS/Linux）：set -a; source .env; set +a
# HEADLESS=false
# HEADLESS_MODE=playwright        # playwright | arg
# HEADLESS_ARG="--headless=new"
# LEGACY_ARGS=true
# NO_VIEWPORT=false
# BYPASS_CSP=false
# STEALTH_SCRIPTS=false
# USER_AGENT=""
# WINDOW_SIZE="1920x1080"         # 例如：1920x1080
# BROWSER_CHANNEL=chrome
# EXECUTABLE_PATH=""              # 指定浏览器可执行文件路径
# USER_DATA_DIR=""                # 覆盖默认 profile 目录
# ANTIBOT_WAIT_MS=60000
# FINGERPRINT=false               # 打印指纹并退出
# COOKIES=false                   # 打印 cookies 并退出
# SLOWMO_MS=""                    # 例如：200
# DEBUG_DIR="debug_cli"           # 出错时 dump 调试信息
#
# 额外请求头（可选）
# ACCEPT_LANGUAGE="zh-CN,zh;q=0.9,en;q=0.8"
# SEC_CH_UA=""
# SEC_CH_UA_MOBILE="?0"
# SEC_CH_UA_PLATFORM="macOS"
